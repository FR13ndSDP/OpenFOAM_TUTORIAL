FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvSolution;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

solvers
{
    beta
    {
        solver		GAMG;
        smoother	DILUGaussSeidel;
        tolerance	1e-6;
        relTol		0;
    }
}

