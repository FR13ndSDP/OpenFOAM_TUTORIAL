FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      beta;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

// Make this a dimensionless scalar
dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    left
    {
        type            fixedValue;
        value           uniform 1;
    }
    
    lower
    {
        type            fixedValue;
        value           uniform 0;
    }
    
    "(upper|right)"
    {
    	type zeroGradient;
	}

    frontAndBack
    {
        type            empty;
    }
}

// ************************************************************************* //
