// 这个文件控制：
// - 湍流模型初始参数
// - 初始流场和边界参数

//==================
// 网格设置

// 管长 [m]
Lpipe 0.68247;
// 管半径 [m]
Rpipe 0.02625;

// cell数目
//	- 沿管长
Nx 50;
//  - 沿径向的内部网格数
NRinner 20;
//	- 沿径向的边界层网格数
NRouter 25;

// cell大小比
expRatWall 0.05;

// 计算全管或者1/2，1/4管
noQuarters 1; // 只计算1/4管

//==================
// 流体性质
visc	1.09e-6;
rho		998.8;

//==================
// 流场信息
pressure	0;
flowVelocity 2;
deltaByR 0.1;

// 入口值
turbulentKE	6.0000E-2; // 湍流动能
turbulentOmega	4.4721E+0; // 湍流动能耗散率


// ************************************************************************* //


