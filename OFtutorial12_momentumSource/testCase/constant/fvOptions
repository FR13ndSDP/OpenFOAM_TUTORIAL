actuationDisk
{
    type customActuationDiskSource;

    upstreamPoint (-1.5 0 0);
    diskDir (-1.0 0.5 0);
    diskCentre (0 0 0);
    D 1;
    thickness 0.1;
    Cp 0.386;
    Ct 0.58;

    fields (U);
}


